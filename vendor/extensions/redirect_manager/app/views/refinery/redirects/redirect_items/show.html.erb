<% content_for :title do %>
  <%= @redirect_item.src_path %>
<% end %>

<section id="cover" style="height: 200px">
</section>

<section id="redirect_item">
  <div class="container">
    <div class="row">
      <div class="sidebar col-xs-12 col-sm-4 col-md-3">
        <ul id="page-sub-nav" class="list-unstyled">
          <% ::Refinery::Redirects::RedirectItem.get_all.each do |redirect_item| %>
            <li><%= link_to redirect_item.src_path, refinery.redirects_redirect_item_path(redirect_item) %></li>
          <% end %>
        </ul>
      </div>
      <div class="col-xs-12 col-sm-8 col-md-9">
        <h1><%= @redirect_item.src_path %></h1>
        <h1><%= @redirect_item.dest_url %></h1>
        <h1><%= @redirect_item.notes %></h1>
        <%=raw @redirect_item.redirect_type %>
      </div>
    </div>
  </div>
</section>
